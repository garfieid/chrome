{
  "manifest_version": 2,

  "name": "iTab",
  "description": "newtab",
  "version": "1.0",

  "icons": {
  	"128": "icon128.png"
  },

  "permissions": [
  	"bookmarks",
    "http://cn.bing.com/",
    "https://cn.bing.com/",
    "chrome://favicon/",
    "chrome://favicon/*"
  ],
  
  "update_url": "http://127.0.0.1/",
  
  //"browser_action": {
  //  "default_icon": "icon.png",
  //  "default_popup": "popup.html"
  //},
  
  "chrome_url_overrides":{
  	"newtab": "mytab.html"
  },
  
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}